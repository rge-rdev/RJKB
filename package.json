    "@types/react-syntax-highlighter": "^15.5.6",
    "autoprefixer": "^10.4.13",
    "cache-loader": "^4.1.0",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "docusaurus-theme-search-typesense": "^0.9.0-3",
    "esbuild-loader": "^3.0.1",
    "fs-extra": "^11.1.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "patch-package": "^6.5.1",
    "postcss": "^8.4.21",
    "postinstall-postinstall": "^2.1.0",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "prism-react-renderer": "^1.3.5",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.9.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.59.3",
    "svgo": "^3.0.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "terser-webpack-plugin": "^5.3.7",
    "thread-loader": "^3.0.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "upi": "yarn upgrade-interactive --latest",
    "upall": "yarn upgrade -D svgo @mdx-js/react webpack @svgr/webpack",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dt": "rm -r ./docs/* & ts-node initDocs.tsx",
    "mem": "set NODE_OPTIONS=--max-old-space-size=16000",
    "id": "yarn mem && ts-node initDocs.tsx",
    "idd": "yarn id && yarn d",
    "idb": "yarn id && yarn db",
    "d": " yarn mem && yarn --offline docusaurus start --no-open --no-minify",
    "db": "yarn mem && yarn --offline docusaurus build && docusaurus serve",
    "b": "yarn --offline docusaurus build",
    "c": "rm -r -f ./docs/* ./static/preview",
    "store ok folders": " ./docs/ai-ml ./docs/CSS ./docs/HTML ./docs/dev-tool ./docs/computer-science",
    "testing folders": "./docs/HTML ",
    "not building": " ./docs/JS",
    "t": "yarn c && yarn id && rm -r -f ./docs/ai-ml ./docs/CSS ./docs/computer-science ./docs/dev-tool ./docs/JS ./docs/React",
    "td": "yarn t && yarn d",
    "tb": "yarn t && yarn db",
    "build": "docusaurus build",
    "wipe": "rm -r ./docs/* && docusaurus clear",
    "serve": "docusaurus serve",
    "rd": "ts-node initDocs.tsx & docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
