    "postcss": "^8.4.21",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "prism-react-renderer": "^1.3.5",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.6",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.9.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.59.3",
    "sharp": "^0.32.0",
    "svgo": "^3.0.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "terser-webpack-plugin": "^5.3.7",
    "thread-loader": "^3.0.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "upi": "yarn upgrade-interactive --latest",
    "upall": "yarn upgrade -D svgo @mdx-js/react webpack @svgr/webpack",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dt": "rm -r ./docs/* & ts-node initDocs.tsx",
    "mem": "node -e \"console.log(new Date().toLocaleString(), 'run Node Heap:', (require('v8').getHeapStatistics().total_available_size / 1024**2).toFixed(1), 'MB')\"",
    "id": "yarn mem && ts-node initDocs.tsx",
    "it": "yarn id && yarn cj && yarn db",
    "d": "yarn pp && yarn mem && yarn --offline docusaurus start --no-open -p 80",
    "db": "yarn pp && yarn mem && yarn --offline docusaurus build && docusaurus serve --no-open -p 80",
    "b": "yarn --offline docusaurus build",
    "c": "rm -r -f ./docs/* ./static/preview ./typesense/*",
    "cj": "rm -r -f ./docs/CSS ./docs/computer-science ./docs/dev-tool ./docs/HTML ./docs/React ./docs/JS/JS-Definition ./docs/JS/JS-Language ./docs/JS/JS-Snippets ./docs/JS/Library ./docs/JS/Nodejs ./docs/JS/TS",
    "store ok folders": " ./docs/ai-ml ./docs/CSS ./docs/HTML ./docs/dev-tool ./docs/computer-science",
    "testing folders": "./docs/CSS ",
    "not building": " ./docs/JS",
    "t": "yarn c && yarn id && rm -r -f ./docs/ai-ml ./docs/computer-science ./docs/dev-tool ./docs/JS ./docs/HTML ./docs/React",
    "td": "yarn t && yarn d",
    "tb": "yarn t && yarn db",
    "build": "docusaurus build",
    "wipe": "yarn c && docusaurus clear",
    "serve": "docusaurus serve",
    "rd": "ts-node initDocs.tsx & docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "pp": "yarn patch-package",
    "ppall": "yarn patch-package docusaurus-theme-search-typesense && yarn patch-package typesense-docsearch-react",
    "rpp": "rm -r -f .docusaurus/ node_modules/.cache/ && yarn ppall && yarn d",
    "bfg": "java -jar bfg-1.14.0.jar"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@svgr/webpack": "^6.2.1"
  }
}
