{
  "name": "rjdb-map",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@docusaurus/core": "^2.3.1",
    "@docusaurus/module-type-aliases": "^2.3.1",
    "@docusaurus/plugin-ideal-image": "^2.3.1",
    "@docusaurus/preset-classic": "^2.3.1",
    "@docusaurus/theme-common": "^2.3.1",
    "@docusaurus/theme-live-codeblock": "^2.3.1",
    "@mdx-js/react": "^1.6.22",
    "@swc/core": "^1.3.35",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.6",
    "autoprefixer": "^10.4.13",
    "cache-loader": "^4.1.0",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "docusaurus-theme-search-typesense": "^0.9.0-3",
    "esbuild-loader": "^3.0.1",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "plugin-image-zoom": "^1.1.0",
    "postcss": "^8.4.21",
    "prism-react-renderer": "^1.3.5",
    "react-icons": "^4.7.1",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "sass": "^1.58.3",
    "svgo": "^3.0.2",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "thread-loader": "^3.0.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "upi": "yarn upgrade-interactive --latest",
    "upall": "yarn upgrade -D svgo @mdx-js/react webpack @svgr/webpack",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dt": "rm -r ./docs/* & ts-node initDocs.tsx",
    "id": "ts-node initDocs.tsx",
    "idd": "ts-node initDocs.tsx && yarn d",
    "d": "setx NODE_OPTIONS \"--max-old-space-size=16000\" && yarn --offline docusaurus start --no-open --no-minify",
    "bd": "setx NODE_OPTIONS \"--max-old-space-size=16000\" && yarn --offline docusaurus build --no-minify",
    "db": "setx NODE_OPTIONS \"--max-old-space-size=16000\" &&  docusaurus build --no-minify --no-minify && docusaurus serve",
    "build": "docusaurus build",
    "clear": "rm -r ./docs/*",
    "wipe": "rm -r ./docs/* && docusaurus clear",
    "serve": "docusaurus serve",
    "rd": "ts-node initDocs.tsx & docusaurus start",
    "scraper": "docker run -it --env-file=.env -e \"CONFIG=$(cat typesense\\config.json | jq -r tostring)\" typesense/docsearch-scraper",
    "health": "curl http://localhost:8108/health",
    "scrape": "docker run -it --env-file=.env -e \"CONFIG=$(cat docsearch.config.json | jq -r tostring)\" typesense/docsearch-scraper",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
